module IPrime;

interface IPrime {
	Integer divide (Int n);
}

interface ISieve {
	Unit sieve();
}

class Sieve(Int bound) implements ISieve {
	Int n = 0;
	IPrime two;
	{
		two = new Prime(2);
		n = 3;
	}
	Unit run() {
		while (n < bound) {
			two!divide(n) ;
			n = n + 1;
		}
	}
}

class Prime (Int p) implements IPrime {
	IPrime next;
	Fut<Int> f;
	Int divide (Int n) {
		if ( (n % p) != 0) {
			if (next != null) {
				this.f = next!divide (n);
				await this.f?;
				this.divide(n);
				println(n);
			}
			else {
				next = new Prime(n);
			}
		}
		return -1;
	}
}

